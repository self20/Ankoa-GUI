#:kivy 1.8.0

# ---------- QUEUE SCREEN ----------
<queue_rmx@Widget>:
    pos_hint: {'center_x': .5, 'center_y': .5}
    size_hint: .97, .96
    spacing: 5
    cols: 1
    canvas:
        Color:
            rgba: .2, .3, .4, .5
        Rectangle:
            size: self.size
            pos: self.pos
